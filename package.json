{
  "name": "track-parser",
  "version": "1.0.0",
  "description": "Web application for automated radio track preparation",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn lint && yarn test:ci && next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "test": "vitest",
    "test:ci": "NODE_ENV=test vitest run",
    "test:coverage": "vitest run --coverage",
    "check": "node scripts/check-ffmpeg.js",
    "migrate:tracks": "tsx scripts/migrate-tracks-to-supabase.ts",
    "migrate:files": "tsx scripts/migrate-files-to-storage.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.39.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "axios": "^1.6.0",
    "basic-ftp": "^5.0.3",
    "dotenv": "^16.4.5",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^11.1.1",
    "iconv-lite": "^0.6.3",
    "music-tempo": "^1.0.3",
    "next": "^16.1.1",
    "node-id3": "^0.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "^5.0.0",
    "wavesurfer.js": "^7.12.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/fs-extra": "^11.0.1",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^16.2.7",
    "netlify-cli": "^23.13.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": "24.x"
  },
  "packageManager": "yarn@4.12.0"
}
